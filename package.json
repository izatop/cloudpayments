{
    "name": "cloudpayments",
    "version": "7.0.0-dev",
    "description": "CloudPayments API for Node.js with typings",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "test:watch": "jest -w",
        "lint": "eslint '{src,test}/**/*.ts'",
        "build": "rimraf dist/* && tsc -p tsconfig.json",
        "watch": "tsc -w",
        "prepublishOnly": "yarn lint && yarn test"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/izatop/cloudpayments.git"
    },
    "keywords": [
        "payments",
        "cloudpayments",
        "typescript",
        "es6",
        "api"
    ],
    "author": "Artur F. Bier <izatop@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/izatop/cloudpayments/issues"
    },
    "homepage": "https://github.com/izatop/cloudpayments#readme",
    "dependencies": {
        "node-fetch": "2.6.6",
        "object-hash": "^2.2.0",
        "qs": "^6.10.3"
    },
    "optionalDependencies": {
        "asn1js": "^2.2.0",
        "pkijs": "^2.2.2"
    },
    "devDependencies": {
        "@types/asn1js": "^2.0.2",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.12",
        "@types/node-fetch": "^2.5.12",
        "@types/node-forge": "^1.0.0",
        "@types/object-hash": "^2.2.1",
        "@types/pkijs": "^0.0.12",
        "@types/qs": "^6.9.7",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "eslint": "^8.7.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^27.4.7",
        "rimraf": "^3.0.0",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    }
}
